<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Sistema Academico</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/site_alt.css" />
    <link rel="shortcut icon" href="~/favicon.png">
</head>
<body style="background-color: rgb(15, 25, 30); color: crimson;" class="body">
    <header>
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Sistema Academico</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse">
                    <ul class="navbar-nav mr-auto">
                        <li class="nav-item"><a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">Inicio</a></li>
                        <li class="nav-item"><a class="nav-link" asp-area="" asp-controller="Categorias" asp-action="">Categorias</a></li>
                        <li class="nav-item"><a class="nav-link" asp-area="" asp-controller="Cursos" asp-action="">Cursos</a></li>
                        <li class="nav-item"><a class="nav-link" asp-area="" asp-controller="Estudiantes" asp-action="">Estudiantes</a></li>
                        <li class="nav-item"><a class="nav-link" asp-area="" asp-controller="Instructores" asp-action="">Instructores</a></li>
                        <li class="nav-item"><a class="nav-link" asp-area="" asp-controller="Asignadores" asp-action="">Asignadores</a></li>
                        <li class="nav-item"><a class="nav-link" asp-area="" asp-controller="Matriculas" asp-action="">Matriculas</a></li>
                        <li class="nav-item"><a class="nav-link" asp-area="" asp-controller="Home" asp-action="Contact">Contacto</a></li>
                        <li class="nav-item"><a class="nav-link" asp-area="" asp-controller="Home" asp-action="About">Acerca de</a></li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - Sistema Academico .NET Core - <a asp-area="" asp-controller="Home" asp-action="About">Acerca de...</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
